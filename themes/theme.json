  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#41505d",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Constant",
      "scope": "constant",
      "settings": {
        "foreground": "#e27e8d"
      }
    },
    {
      "name": "Values",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#cb9be3"
      }
    },
    {
      "name": "Entity",
      "scope": "entity",
      "settings": {
        "foreground": "#68A1F0"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "foreground": "#d95468"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": {
        "foreground": "#5ec4ff"
      }
    },
    {
      "name": "Storage type",
      "scope": "storage.type.class.js",
      "settings": {
        "foreground": "#008b94"
      }
    },
    {
      "name": "Meta",
      "scope": "meta",
      "settings": {
        "foreground": "#718CA1"
      }
    },
    {
      "name": "Meta Brace",
      "scope": "meta.brace",
      "settings": {
        "foreground": "#718CA1"
      }
    },
    {
      "name": "Punctuation",
      "scope": "punctuation",
      "settings": {
        "foreground": "#718CA1"
      }
    },
    {
      "name": "Punctuation Parameters",
      "scope": "punctuation.definition.parameters",
      "settings": {
        "foreground": "#718CA1"
      }
    },
    {
      "name": "Property Parameters",
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#B7C5D3"
      }
    },
    {
      "name": "Function Parameters",
      "scope": [
        "entity.name.variable.parameter",
        "meta.at-rule.function variable",
        "meta.at-rule.mixin variable",
        "meta.function.arguments",
        "meta.selectionset.graphql meta.arguments.graphql variable.arguments.graphql"
      ],
      "settings": {
        "foreground": "#ebbf83"
      }
    },
    {
      "name": "Punctuation Template Expression",
      "scope": "punctuation.definition.template-expression",
      "settings": {
        "foreground": "#68A1F0"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "foreground": "#008B94"
      }
    },
    {
      "name": "Storage Type Arrow Function",
      "scope": "storage.type.function.arrow",
      "settings": {
        "foreground": "#008B94"
      }
    },
    {
      "name": "String",
      "scope": ["string", "punctuation.definition.string"],
      "settings": {
        "foreground": "#68A1F0"
      }
    },
    {
      "name": "String Template",
      "scope": ["string.template", "punctuation.definition.string.template"],
      "settings": {
        "foreground": "#68f0e9"
      }
    },
    {
      "name": "Support",
      "scope": "support",
      "settings": {
        "foreground": "#718CA1"
      }
    },

    {
      "name": "[Lua] Function",
      "scope": [
        "support.function.wow.lua",
        "support.function.wow.string.library.lua",
        "support.function.wow.math.library.lua",
        "support.function.wow.table.library.lua",
        "support.function.wow.bit.library.lua",
        "support.function.wow.coroutine.library.lua"
      ],
      "settings": {
        "foreground": "#36bf7f"
      }
    },

    {
      "name": "[Lua] Method",
      "scope": ["support.function.any-method.lua", "entity.name.function.lua"],
      "settings": {
        "foreground": "#70e1e8"
      }
    },

    {
      "name": "[Lua] Variable",
      "scope": ["variable.other.lua", "entity.other.attribute.lua"],
      "settings": {
        "foreground": "#7faca6"
      }
    },

    {
      "name": "[Lua] Class",
      "scope": ["entity.name.class.lua"],
      "settings": {
        "foreground": "#6992ea"
      }
    },

    {
      "name": "[Lua] Parameter",
      "scope": ["variable.parameter.function.lua"],
      "settings": {
        "foreground": "#e8e9dc"
      }
    },

    {
      "name": "[Lua] Variable Self",
      "scope": ["variable.language.self.lua"],
      "settings": {
        "foreground": "#f99654"
      }
    },

    {
      "name": "[Lua] Constant Numeric",
      "scope": ["constant.numeric.float.lua"],
      "settings": {
        "foreground": "#bb7eef"
      }
    },

    {
      "name": "[Lua] String",
      "scope": ["string.quoted.single.lua", "string.quoted.double.lua"],
      "settings": {
        "foreground": "#f5e392"
      }
    },

    {
      "name": "[Lua] Constant",
      "scope": ["constant.language.lua"],
      "settings": {
        "foreground": "#ed4848"
      }
    },

    {
      "name": "[Lua] Annotation",
      "scope": ["storage.type.annotation.lua"],
      "settings": {
        "foreground": "#798d9c"
      }
    },

    {
      "name": "[Lua] italic",
      "scope": [
        "keyword.local.lua",
        "keyword.control.lua",
        "keyword.operator.lua"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "[Lua] bold",
      "scope": [
        "support.function.wow.lua",
        "support.function.wow.string.library.lua",
        "support.function.wow.math.library.lua",
        "support.function.wow.table.library.lua",
        "support.function.wow.bit.library.lua",
        "support.function.wow.coroutine.library.lua",
        "entity.name.function.lua",
        "support.function.any-method.lua"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    }
  ]
}
