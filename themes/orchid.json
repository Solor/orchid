	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#4b5d6c",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Constant",
			"scope": "constant",
			"settings": {
				"foreground": "#e27e8d"
			}
		},
		{
			"name": "Values",
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#cb9be3"
			}
		},
		{
			"name": "Entity",
			"scope": "entity",
			"settings": {
				"foreground": "#68A1F0"
			}
		},
		{
			"name": "Invalid",
			"scope": "invalid",
			"settings": {
				"foreground": "#d95468"
			}
		},
		{
			"name": "Keyword",
			"scope": "keyword",
			"settings": {
				"foreground": "#5ec4ff"
			}
		},
		{
			"name": "Storage type",
			"scope": "storage.type.class.js",
			"settings": {
				"foreground": "#008b94"
			}
		},
		{
			"name": "Meta",
			"scope": "meta",
			"settings": {
				"foreground": "#718CA1"
			}
		},
		{
			"name": "Meta Brace",
			"scope": "meta.brace",
			"settings": {
				"foreground": "#718CA1"
			}
		},
		{
			"name": "Punctuation",
			"scope": "punctuation",
			"settings": {
				"foreground": "#718CA1"
			}
		},
		{
			"name": "Punctuation Parameters",
			"scope": "punctuation.definition.parameters",
			"settings": {
				"foreground": "#718CA1"
			}
		},
		{
			"name": "Property Parameters",
			"scope": "variable.parameter",
			"settings": {
				"foreground": "#B7C5D3"
			}
		},
		{
			"name": "Function Parameters",
			"scope": [
				"entity.name.variable.parameter",
				"meta.at-rule.function variable",
				"meta.at-rule.mixin variable",
				"meta.function.arguments",
				"meta.selectionset.graphql meta.arguments.graphql variable.arguments.graphql"
			],
			"settings": {
				"foreground": "#ebbf83"
			}
		},
		{
			"name": "Punctuation Template Expression",
			"scope": "punctuation.definition.template-expression",
			"settings": {
				"foreground": "#68A1F0"
			}
		},
		{
			"name": "Storage",
			"scope": "storage",
			"settings": {
				"foreground": "#008B94"
			}
		},
		{
			"name": "Storage Type Arrow Function",
			"scope": "storage.type.function.arrow",
			"settings": {
				"foreground": "#008B94"
			}
		},
		{
			"name": "String",
			"scope": [
				"string",
				"punctuation.definition.string"
			],
			"settings": {
				"foreground": "#68A1F0"
			}
		},
		{
			"name": "String Template",
			"scope": [
				"string.template",
				"punctuation.definition.string.template"
			],
			"settings": {
				"foreground": "#68f0e9"
			}
		},
		{
			"name": "Support",
			"scope": "support",
			"settings": {
				"foreground": "#718CA1"
			}
		},
		{
			"name": "Support Function",
			"scope": [
				"support.function"
			],
			"settings": {
				"foreground": "#70e1e8"
			}
		},
		{
			"name": "[LUA] - italic",
			"scope": [
				"comment.line.double-dash.lua",
				"punctuation.definition.comment.lua",
				"storage.type.function.lua",
				"storage.modifier.local.lua",
				"storage.type.function.property.lua",
				"keyword.control.function-end.lua",
				"keyword.control.lua",
				"keyword.operator.logical.lua",
				"storage.type.function.anonymous.lua"
			],
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "[LUA] - bold",
			"scope": [
				"entity.name.function.lua",
				"meta.function-call.lua",
				// "constant.language.boolean.false.lua",
				// "constant.language.boolean.true.lua",
				// "constant.language.lua",
				// "keyword.operator.logic.lua"
			],
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "[LUA] - Local Functions",
			"scope": "meta.function.lua",
			"settings": {
				"foreground": "#45888d"
			}
		},
		// {
		// 	"name": "[LUA] - Method Functions",
		// 	"scope": "methods.local.lua",
		// 	"settings": {
		// 		"foreground": "#9889eb"
		// 	}
		// },
		{
			"name": "[LUA] - Values",
			"scope": [
				"constant.language.boolean.false.lua",
				"constant.language.boolean.true.lua"
			],
			"settings": {
				"foreground": "#cb9be3"
			}
		},
		// {
		// 	"name": "[LUA] - self",
		// 	"scope": ["variable.language.lua"],
		// 	"settings": {
		// 		"foreground": "#ebbf83"
		// 	}
		// },
		{
			"name": "[LUA] - variable.other.table.property",
			"scope": [
				"variable.other.table.property.lua"
			],
			"settings": {
				"foreground": "#8babc4"
			}
		},
		// {
		// 	"name": "[LUA] - meta.function-call",
		// 	"scope": [
		// 		"meta.function-call.lua"
		// 	],
		// 	"settings": {
		// 		"foreground": "#70e8a6"
		// 	}
		// },
		{
			"name": "[LUA] - keyword.operator.logic",
			"scope": [
				"keyword.operator.logic.lua"
			],
			"settings": {
				"foreground": "#ffc75e"
			}
		},
		{
			"name": "[LUA] - entity.name.function",
			"scope": [
				"entity.name.function.lua"
			],
			"settings": {
				"foreground": "#68A1F0"
			}
		},
		{
			"name": "[LUA] - entity.name.function-table",
			"scope": [
				"entity.name.function-table.lua"
			],
			"settings": {
				"foreground": "#3c7fdd"
			}
		},
		{
			"name": "[LUA] - string.quoted",
			"scope": [
				"string.quoted.single.lua",
				"string.quoted.double.lua",
				"string.quoted.block.lua"
			],
			"settings": {
				"foreground": "#37c9b0"
			}
		},
		{
			"name": "[LUA] - variable.other",
			"scope": [
				"variable.other.lua",
				"variable.language.lua"
			],
			"settings": {
				"foreground": "#8babc4"
			}
		},
		{
			"name": "[LUA] - variable.other.table.property",
			"scope": [
				"variable.other.table.property.lua"
			],
			"settings": {
				"foreground": "#74b5c7"
			}
		},
		{
			"name": "[LUA] - constant.language",
			"scope": [
				"constant.language.lua"
			],
			"settings": {
				"foreground": "#f3788b"
			}
		},
		{
			"name": "[LUA] - constant.numeric",
			"scope": [
				"constant.numeric.lua"
			],
			"settings": {
				"foreground": "#cf84f5"
			}
		},
		{
			"name": "[LUA] - keyword.control",
			"scope": [
				"keyword.control.lua",
				"keyword.control.function-end.lua",
				"storage.type.function.property.lua",
				"storage.type.function.lua",
				"storage.type.function.anonymous.lua"
			],
			"settings": {
				"foreground": "#39cce6"
			}
		},
	]
}